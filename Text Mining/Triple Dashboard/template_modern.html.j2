<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ report_title }}</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&family=Source+Code+Pro:wght@400;500&display=swap" rel="stylesheet">
    <style>
        :root {
            --color-background: #FDFCF9;
            --color-background-subtle: #F7F7F8;
            --color-text: #1D2029;
            --color-text-secondary: #5F6368;
            --color-border: #EAEBEB;
            --color-border-subtle: #CBD6E4;
            --color-border-hard: #7C828A;
            --color-accent: #635BFF;
            --color-accent-new: #E87900;
            --color-entity-rare: #407a8dcf;
            --color-arrow: #3638E8;
            --color-active-filter-bg: #E4E2FF;
            --font-primary: 'Inter', sans-serif;
            --font-mono: 'Source Code Pro', monospace;
            --border-radius: 8px;
            --shadow-subtle: 0 4px 6px rgba(95, 99, 104, 0.06);
        }
        html, body {
            height: 100%;
            margin: 0;
        }
        body {
            font-family: var(--font-primary);
            background-color: var(--color-background);
            color: var(--color-text);
            padding: 2rem 0 0 0;
            line-height: 1.6;
            -webkit-font-smoothing: antialiased;
            display: flex;
            flex-direction: column;
        }
        .container {
            width: min(100% - 4rem, 160rem);
            margin: 0 auto;
            display: flex;
            flex-direction: column;
            flex-grow: 1;
        }
        h1 {
            padding-top: 1rem;
            margin-bottom: 2rem;
            flex-shrink: 0;
        }
        h2 {
            font-size: 1.25rem;
            font-weight: 700;
            margin-top: 0;
            margin-bottom: 1.1rem;
        }
        h3 {
            font-size: 1.1rem;
            font-weight: 600;
            margin-top: 0;
            margin-bottom: 1rem;
        }
        h4 {
            font-size: 1rem;
            font-weight: 600;
            margin: 1.75rem 0 1rem 0;
            padding-bottom: 0.75rem;
            border-bottom: 1px solid var(--color-border);
        }
        .global-summary-card {
            display: flex;
            flex-wrap: wrap;
            gap: 1.5rem;
            padding: 1rem 0.5rem;
            justify-content: space-around;
            align-items: stretch;
            margin-bottom: 2rem;
        }
        .global-summary-card .stat-item {
            text-align: center;
            background-color: var(--color-background);
            padding: 1.25rem 2rem;
            border-radius: var(--border-radius);
            flex-grow: 1;
            border: 1px solid var(--color-border);
        }
        .stat-item .value {
            font-size: 2.25rem;
            font-weight: 700;
            line-height: 1.2;
            color: var(--color-accent);
        }
        .stat-item .label {
            font-size: 0.9rem;
            font-weight: 500;
            color: var(--color-text-secondary);
        }
        .sidebar-panel {
            background-color: var(--color-background-subtle);
            border: 1px solid var(--color-border);
            border-radius: var(--border-radius);
            padding: 1.25rem 1.5rem;
            margin-bottom: 1.5rem;
        }
        #analytics-panel {
             background-color: transparent;
             border: none;
             padding: 0;
        }
        #analytics-panel .summary-section {
            background-color: var(--color-background-subtle);
            border: 1px solid var(--color-border);
            border-radius: var(--border-radius);
            padding: 1.25rem 1.5rem;
            margin-bottom: 1.5rem;
        }
        .text-search-input {
            width: 100%;
            padding: 0.6rem 0.8rem;
            font-size: 1rem;
            border-radius: 6px;
            border: 1px solid var(--color-border-subtle);
            margin-bottom: 1.5rem;
            font-family: var(--font-primary);
            transition: border-color 0.2s ease, box-shadow 0.2s ease;
        }
        .text-search-input:focus {
            outline: none;
            border-color: var(--color-accent);
            box-shadow: 0 0 0 3px var(--color-active-filter-bg);
        }
        #filter-status-content h4 {
            font-size: 0.9rem;
            margin: 0 0 0.5rem 0;
            padding: 0;
            border: none;
        }
        .stats-list { padding-left: 0; margin: 0; list-style: none; font-size: 0.9rem; max-height: 250px; overflow-y: auto; }
        .stats-list li { display: flex; justify-content: space-between; padding: 0.4rem 0.5rem; border-radius: 4px; margin-bottom: 0.25rem; align-items: center; }
        .stats-list li:nth-child(odd) { background-color: #FFFFFF; }
        .stats-list code, .stats-list .code { font-family: var(--font-mono); background: var(--color-border); color: var(--color-text); padding: 2px 6px; border-radius: 4px; font-size: 0.85rem; }
        .stats-list .pattern-rel { color: var(--color-arrow); font-weight: 500; }
        .empty-state { color: var(--color-text-secondary); font-style: italic; font-size: 0.9rem; }
        .two-column-layout { display: flex; align-items: flex-start; gap: 2rem; flex-grow: 1; overflow: hidden; height: 100%; }
        .main-content-column, .sidebar-column { flex: 1; min-width: 0; overflow-y: auto; height: 100%; padding-right: 0.5rem; }
        @media (max-width: 1024px) { body { height: auto; } .container { width: min(100% - 2rem, 160rem); } .two-column-layout { flex-direction: column; overflow: visible; height: auto; } .main-content-column, .sidebar-column { overflow-y: visible; padding-right: 0; height: auto; } .sidebar-column { order: -1; } }
        .hidden-by-filter { display: none !important; }
        #filter-status { display: none; margin-bottom: 1.5rem; }
        #filter-status-content > div:not(:last-child) { margin-bottom: 1rem; }
        #clear-all-filters-btn { font-weight: 600; color: var(--color-accent); background: none; border: 1px solid var(--color-accent); border-radius: 6px; padding: 0.5rem 1rem; cursor: pointer; font-size: 0.9rem; transition: all 0.2s ease; }
        #clear-all-filters-btn:hover { background-color: var(--color-accent); color: white; }
        .filterable-badge { cursor: pointer; transition: all 0.2s ease; }
        .filterable-badge:hover { transform: translateY(-2px); }
        .filterable-badge.selected { box-shadow: 0 0 0 2px #1D2029, 0 0 0 5px var(--color-active-filter-bg); transform: scale(1.05) translateY(-2px); }
        .patterns-column li.active-filter { background-color: var(--color-active-filter-bg); border-radius: 4px; }
        .patterns-column li:not(.active-filter):hover { background-color: #f0f0f0; border-radius: 4px; }
        .patterns-column li { cursor: pointer; }
        .badge-container { display: flex; flex-wrap: wrap; gap: 0.5rem 0.75rem; }
        .entity-type-badge { display: inline-block; color: white; padding: 0.25rem 0.75rem; border-radius: 1rem; font-size: 0.8rem; font-weight: 500; white-space: nowrap; }
        .badge-freq-high { background-color: var(--color-accent); }
        .badge-freq-medium { background-color: var(--color-accent-new); }
        .badge-freq-low { background-color: var(--color-entity-rare); }
        .legend-container { display: flex; flex-wrap: wrap; gap: 1rem; margin-bottom: 1.25rem; padding-bottom: 1.25rem; border-bottom: 1px solid var(--color-border); font-size: 0.85rem; align-items: center; }
        .legend-item { display: flex; align-items: center; gap: 0.5rem; color: var(--color-text-secondary); font-weight: 500; }
        .legend-color-box { width: 14px; height: 14px; border-radius: 3px; }
        .patterns-container { display: flex; gap: 2rem; align-items: flex-start; }
        .patterns-column ol { margin: 0; padding-left: 0; list-style: none; font-size: 0.9rem; }
        .patterns-column li { margin-bottom: 0.5rem; color: var(--color-text-secondary); display: flex; justify-content: space-between; align-items: baseline; gap: 1rem; padding: 0.25rem 0.5rem; }
        .patterns-column li .rank { font-weight: 600; color: var(--color-text-secondary); min-width: 1.5rem; text-align: right; margin-right: 0.5rem; }
        .patterns-column li .pattern { font-family: var(--font-mono); color: var(--color-text); }
        .patterns-column li .count { font-weight: 600; background: var(--color-border); padding: 1px 6px; border-radius: 4px; font-size: 0.8rem; }
        .patterns-column .pattern-rel { color: var(--color-arrow); font-weight: 500; }
        .toggle-button { display: block; width: 100%; margin-top: 1rem; padding: 0.5rem; font-family: var(--font-primary); font-size: 0.9rem; font-weight: 500; background: none; border: 1px solid var(--color-border); border-radius: 6px; cursor: pointer; transition: all 0.2s ease; }
        .toggle-button:hover { background-color: var(--color-background-subtle); border-color: var(--color-text-secondary); }
        .speaker-turn { margin-bottom: 1.5rem; }
        .turn-header { display: flex; justify-content: space-between; align-items: baseline; margin-bottom: 1.5rem; padding-bottom: 1rem; border-bottom: 1.4px solid var(--color-border-hard); }
        .turn-speaker-name { font-size: 1.5rem; font-weight: 700; }
        .turn-meta { font-size: 0.9rem; color: var(--color-text-secondary); }
        .extraction-card { background-color: var(--color-background); border: 1px solid var(--color-border); border-radius: var(--border-radius); margin-bottom: 1.5rem; padding: 1.5rem; box-shadow: var(--shadow-subtle); transition: transform 0.2s ease-in-out, box-shadow 0.2s ease-in-out; }
        .extraction-card:hover { transform: translateY(-3px); }
        .sro-triple { display: flex; align-items: center; justify-content: space-between; gap: 1rem; margin-bottom: 1.5rem; }
        .entity-box { border: 1px solid var(--color-border-subtle); border-radius: var(--border-radius); padding: 1rem 1.25rem; flex-grow: 1; flex-basis: 0; background-color: var(--color-background-subtle); display: flex; justify-content: center; align-items: baseline; gap: 0.6rem; }
        .entity-name { font-size: 1.1rem; font-weight: 600; }
        .relation-link { display: flex; flex-direction: column; align-items: center; text-align: center; flex-shrink: 0; color: var(--color-text-secondary); padding: 0 0.5rem; }
        .relation-semantic-form { font-size: 0.85rem; margin-bottom: 0.25rem; }
        .relation-arrow { font-size: 1.5rem; font-weight: bold; color: var(--color-accent); }
        .relation-details { margin-bottom: 1.5rem; font-size: 0.9rem; color: var(--color-text-secondary); }
        .relation-details code { font-family: var(--font-mono); background: var(--color-background-subtle); border: 1px solid var(--color-border); color: var(--color-text); padding: 2px 5px; border-radius: 4px; font-size: 0.85rem; }
        .evidence-section { border-top: 1px solid var(--color-border); padding-top: 1.5rem; }
        .evidence-text { border-left: 3px solid var(--color-accent); padding: 0.25rem 1.25rem; margin: 0 0 1rem 0; font-style: italic; color: var(--color-text-secondary); }
        .evidence-sources { text-align: right; }
        .evidence-id-tag { display: inline-block; font-size: 0.8rem; font-style: normal; background-color: var(--color-background-subtle); color: var(--color-text-secondary); border: 1px solid var(--color-border); padding: 0.2rem 0.6rem; margin-left: 0.5rem; border-radius: 4px; transition: all 0.2s ease; }
        .evidence-id-tag:hover { background-color: var(--color-accent); border-color: var(--color-accent); color: white; cursor: pointer; }
        .section-header-with-actions { display: flex; justify-content: space-between; align-items: center; margin-bottom: 1.1rem; }
        .export-csv-btn { font-size: 0.8rem; font-weight: 500; color: var(--color-text-secondary); background: white; border: 1px solid var(--color-border); border-radius: 6px; padding: 0.35rem 0.75rem; cursor: pointer; transition: all 0.2s ease; display: inline-flex; align-items: center; gap: 0.4rem; }
        .export-csv-btn:hover { background-color: var(--color-background-subtle); border-color: var(--color-text-secondary); color: var(--color-text); }
        .export-csv-btn svg { width: 14px; height: 14px; stroke: currentColor; stroke-width: 2; fill: none; }
        .collapsible-header { display: flex; justify-content: space-between; align-items: center; cursor: pointer; }
        .collapsible-header h2 { margin-bottom: 0; }
        .collapsible-content { max-height: 1500px; overflow: hidden; transition: max-height 0.4s ease-in-out, margin-top 0.4s ease-in-out, opacity 0.3s ease-in-out; margin-top: 1.25rem; opacity: 1; }
        .collapsible.collapsed .collapsible-content { max-height: 0; margin-top: 0; opacity: 0; }
        .chevron { border-color: var(--color-text-secondary); border-style: solid; border-width: 0 2px 2px 0; display: inline-block; padding: 3px; transform: rotate(45deg); transition: transform 0.3s ease-in-out; }
        .collapsible.collapsed .chevron { transform: rotate(-135deg); }
        .filterable-relation { cursor: pointer; transition: all 0.2s ease; display: inline-block; }
        .filterable-relation:hover { background-color: var(--color-active-filter-bg); }
        .filterable-relation.selected { box-shadow: 0 0 0 2px var(--color-text), 0 0 0 4px var(--color-active-filter-bg); transform: scale(1.05); background-color: var(--color-background); }
    </style>
</head>
<body>
    <div class="container">
        <header><h1>{{ report_title }}</h1></header>
        
        <div class="two-column-layout">
            <div class="main-content-column">
                <div class="summary-section global-summary-card">
                    <div class="stat-item">
                        <div class="value">{{ global_stats.total_extractions }}</div>
                        <div class="label">Total Extractions</div>
                    </div>
                     <div class="stat-item">
                        <div class="value">{{ global_stats.total_speaker_turns }}</div>
                        <div class="label">Speaker Turns</div>
                    </div>
                    <div class="stat-item">
                        <div class="value">{{ global_stats.unique_entity_names }}</div>
                        <div class="label">Unique Entities</div>
                    </div>
                    <div class="stat-item">
                        <div class="value">{{ global_stats.unique_entity_types }}</div>
                        <div class="label">Unique Entity Types</div>
                    </div>
                    <div class="stat-item">
                        <div class="value">{{ global_stats.unique_relations }}</div>
                        <div class="label">Unique Relations</div>
                    </div>
                </div>
                <main>
                    {% for turn in speaker_turns %}
                    <div class="speaker-turn">
                        <div class="turn-header">
                            <span class="turn-speaker-name">{{ turn.speaker_name }}</span>
                            <span class="turn-meta">{{ turn.role }} | Utterance #{{ turn.utterance_order }} | {{ turn.extraction_count }} Extractions</span>
                        </div>
                        
                        {% for extraction in turn.extractions %}
                        <div class="extraction-card" 
                             data-subject-type="{{ extraction.subject_entity.entity_type }}"
                             data-subject-name="{{ extraction.subject_entity.name | lower }}"
                             data-relation-form="{{ extraction.relation.semantic_form }}"
                             data-object-type="{{ extraction.object_entity.entity_type }}"
                             data-object-name="{{ extraction.object_entity.name | lower }}">

                            <div class="sro-triple">
                                <div class="entity-box subject">
                                    <span class="entity-name">{{ extraction.subject_entity.name }}</span>
                                    {% set subj_utt_count = entity_utterance_counts_map.get(extraction.subject_entity.entity_type, 0) %}
                                    <span class="entity-type-badge {% if subj_utt_count > 3 %}badge-freq-high{% elif subj_utt_count >= 2 %}badge-freq-medium{% else %}badge-freq-low{% endif %}">
                                        {{ extraction.subject_entity.entity_type }}
                                    </span>
                                </div>
                                <div class="relation-link"><span class="relation-semantic-form">{{ extraction.relation.semantic_form }}</span><span class="relation-arrow">&rarr;</span></div>
                                <div class="entity-box object">
                                    <span class="entity-name">{{ extraction.object_entity.name }}</span>
                                    {% set obj_utt_count = entity_utterance_counts_map.get(extraction.object_entity.entity_type, 0) %}
                                    <span class="entity-type-badge {% if obj_utt_count > 3 %}badge-freq-high{% elif obj_utt_count >= 2 %}badge-freq-medium{% else %}badge-freq-low{% endif %}">
                                        {{ extraction.object_entity.entity_type }}
                                    </span>
                                </div>
                            </div>
                            <div class="relation-details">Surface Form: <code>"{{ extraction.relation.surface_form }}"</code></div>
                            <div class="evidence-section">
                                <h3>Evidence Source</h3>
                                <blockquote class="evidence-text">{{ extraction.evidence_text }}</blockquote>
                                <div class="evidence-sources">{% for source_id in extraction.evidence_sources %}<span class="evidence-id-tag">{{ source_id }}</span>{% endfor %}</div>
                            </div>
                        </div>
                        {% else %}
                        <div class="extraction-card" style="text-align: center; color: var(--color-text-secondary);"><p>No extractions found for this utterance.</p></div>
                        {% endfor %}
                    </div>
                    {% else %}
                    <div class="extraction-card" style="text-align: center; color: var(--color-text-secondary);"><p>No speaker turns found in the provided data.</p></div>
                    {% endfor %}
                </main>
            </div>

            <div class="sidebar-column" id="sidebar">
                <div id="filter-status" class="sidebar-panel">
                    <div style="display: flex; justify-content: space-between; align-items: flex-start;">
                        <div id="filter-status-content">
                            <div id="search-filter-display" style="display: none;">
                                <h4>Searching for:</h4>
                                <span id="search-filter-text"></span>
                            </div>
                            <div id="type-filter-display" style="display: none;">
                                <h4>Filtering by Type(s):</h4>
                                <div class="badge-container" id="type-filter-tags"></div>
                            </div>
                            <div id="relation-filter-display" style="display: none;">
                                <h4>Filtering by Relation:</h4>
                                <div id="relation-filter-tags"></div>
                            </div>
                            <div id="pattern-filter-display" style="display: none;">
                                <h4>Filtering by Pattern:</h4>
                                <span id="pattern-filter-text"></span>
                            </div>
                        </div>
                        <button id="clear-all-filters-btn">Clear All</button>
                    </div>
                </div>

                <div class="sidebar-panel" id="filter-and-search-panel">
                    <h2>Filter & Search</h2>
                    <input type="search" id="entity-search-input" placeholder="Search entity names..." class="text-search-input">
                    
                    {% if all_entity_types %}
                    <div>
                        <div class="section-header-with-actions">
                            <h3>Entity Types (Click to Filter)</h3>
                            <button id="export-entity-csv-btn" class="export-csv-btn" title="Export entity type data as CSV">
                                <svg viewBox="0 0 24 24"><path d="M21 15v4a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2v-4"></path><polyline points="7 10 12 15 17 10"></polyline><line x1="12" y1="15" x2="12" y2="3"></line></svg>
                                Export CSV
                            </button>
                        </div>
                        <div class="legend-container">
                            <div class="legend-item"><div class="legend-color-box" style="background-color: var(--color-accent);"></div><span>&gt; 3 Utterances</span></div>
                            <div class="legend-item"><div class="legend-color-box" style="background-color: var(--color-accent-new);"></div><span>2-3 Utterances</span></div>
                            <div class="legend-item"><div class="legend-color-box" style="background-color: var(--color-entity-rare);"></div><span>1 Utterance</span></div>
                        </div>
                        <div class="badge-container" id="entity-type-filter-container">
                            {% for type_info in all_entity_types %}
                            <span class="entity-type-badge filterable-badge {% if type_info.utterance_count > 3 %}badge-freq-high{% elif type_info.utterance_count >= 2 %}badge-freq-medium{% else %}badge-freq-low{% endif %}" 
                                  data-entity-type="{{ type_info.name }}"
                                  data-total-count="{{ type_info.count }}"
                                  data-utterance-count="{{ type_info.utterance_count }}">
                                  {{ type_info.name }} [{{ type_info.count }}]
                            </span>
                            {% endfor %}
                        </div>
                        <button id="toggle-entity-types-btn" class="toggle-button" style="display: none;">Show More</button>
                    </div>
                    {% endif %}
                </div>

                <div id="analytics-panel">
                    <h2>Pattern Analytics</h2>
                    <div class="summary-section collapsible collapsed">
                        <div class="collapsible-header">
                            <h3>Entity Type Patterns</h3>
                            <span class="chevron"></span>
                        </div>
                        <div class="collapsible-content">
                            <h4>Multi-Typed Entities ({{ multi_typed_entities|length }} found)</h4>
                            <ul class="stats-list">
                                {% for name, types in multi_typed_entities.items() %}
                                <li>
                                    <span>{{ name }}</span> 
                                    <div class="badge-container">
                                        {% for type in types %}
                                            {% set utt_count = entity_utterance_counts_map.get(type, 0) %}
                                            <span class="entity-type-badge {% if utt_count > 3 %}badge-freq-high{% elif utt_count >= 2 %}badge-freq-medium{% else %}badge-freq-low{% endif %}">{{ type }}</span>
                                        {% endfor %}
                                    </div>
                                </li>
                                {% else %}<li class="empty-state" style="justify-content: flex-start;">None found.</li>{% endfor %}
                            </ul>
                            <h4>Subject-Only Entity Types ({{ subject_only_types|length }} types)</h4>
                            <div class="badge-container">
                                {% for type in subject_only_types %}{% set utt_count = entity_utterance_counts_map.get(type, 0) %}{% set total_count = entity_total_counts_map.get(type, 0) %}<span class="entity-type-badge filterable-badge {% if utt_count > 3 %}badge-freq-high{% elif utt_count >= 2 %}badge-freq-medium{% else %}badge-freq-low{% endif %}" data-entity-type="{{ type }}" data-total-count="{{ total_count }}" data-utterance-count="{{ utt_count }}">{{ type }} [{{ total_count }}]</span>{% else %}<span class="empty-state">None found.</span>{% endfor %}
                            </div>
                            <h4>Object-Only Entity Types ({{ object_only_types|length }} types)</h4>
                            <div class="badge-container">
                                {% for type in object_only_types %}{% set utt_count = entity_utterance_counts_map.get(type, 0) %}{% set total_count = entity_total_counts_map.get(type, 0) %}<span class="entity-type-badge filterable-badge {% if utt_count > 3 %}badge-freq-high{% elif utt_count >= 2 %}badge-freq-medium{% else %}badge-freq-low{% endif %}" data-entity-type="{{ type }}" data-total-count="{{ total_count }}" data-utterance-count="{{ utt_count }}">{{ type }} [{{ total_count }}]</span>{% else %}<span class="empty-state">None found.</span>{% endfor %}
                            </div>
                        </div>
                    </div>
                    
                    <div class="summary-section collapsible collapsed">
                        <div class="collapsible-header">
                            <h3>Relation Cardinality Patterns</h3>
                            <span class="chevron"></span>
                        </div>
                        <div class="collapsible-content">
                            <h4>One-to-One (by Type)</h4>
                            <ul class="stats-list">
                                {% for rel, patterns in one_to_one_relations.items() %}<li><code class="pattern-rel filterable-relation" data-relation-form="{{ rel }}">{{ rel }}</code><span>{{ patterns|length }} pattern(s), e.g., ({{ patterns[0][0] }})</span></li>{% else %}<li class="empty-state">No self-relations (e.g., PERSON &rarr; PERSON) found.</li>{% endfor %}
                            </ul>
                            <h4>One-to-Many Relations</h4>
                            <ul class="stats-list">
                                {% for rel in one_to_many_relations %}<li><code class="pattern-rel filterable-relation" data-relation-form="{{ rel }}">{{ rel }}</code><span>(1:N Cardinality)</span></li>{% else %}<li class="empty-state" style="justify-content: flex-start;">None found.</li>{% endfor %}
                            </ul>
                            <h4>Many-to-One Relations</h4>
                            <ul class="stats-list">
                                {% for rel in many_to_one_relations %}<li><code class="pattern-rel filterable-relation" data-relation-form="{{ rel }}">{{ rel }}</code><span>(N:1 Cardinality)</span></li>{% else %}<li class="empty-state" style="justify-content: flex-start;">None found.</li>{% endfor %}
                            </ul>
                            <h4>Top {{ top_diverse_relations|length }} Relations by Domain/Range Diversity</h4>
                            <ul class="stats-list">
                                {% for item in top_diverse_relations %}<li><code class="pattern-rel filterable-relation" data-relation-form="{{ item.rel }}">{{ item.rel }}</code> <span>{{ item.domain_size }} Subj Type(s) / {{ item.range_size }} Obj Type(s)</span></li>{% else %}<li class="empty-state">Not enough data to analyze diversity.</li>{% endfor %}
                            </ul>
                        </div>
                    </div>

                    {% if most_frequent_patterns %}
                    <div class="summary-section patterns-column collapsible collapsed">
                        <div class="collapsible-header">
                            <h3>Frequent Structural Patterns</h3>
                            <span class="chevron"></span>
                        </div>
                        <div class="collapsible-content">
                            <div class="patterns-container">
                                <div>
                                    <h4>Top 150 Most Frequent</h4>
                                    <ol id="most-frequent-list">
                                        {% for pattern, count in most_frequent_patterns %}<li data-filter-subject="{{ pattern[0] }}" data-filter-relation="{{ pattern[1] }}" data-filter-object="{{ pattern[2] }}"><span class="rank">{{ loop.index }}.</span><span class="pattern">{{ pattern[0] }} &rarr; <span class="pattern-rel">{{ pattern[1] }}</span> &rarr; {{ pattern[2] }}</span><span class="count">{{ count }}</span></li>{% endfor %}
                                    </ol>
                                    {% if most_frequent_patterns|length > 25 %}<button class="toggle-button" data-target="most-frequent-list">Show All</button>{% endif %}
                                </div>
                                <div>
                                    <h4>Top 150 Least Frequent</h4>
                                    <ol id="least-frequent-list">
                                        {% for pattern, count in least_frequent_patterns %}<li data-filter-subject="{{ pattern[0] }}" data-filter-relation="{{ pattern[1] }}" data-filter-object="{{ pattern[2] }}"><span class="rank">{{ loop.index }}.</span><span class="pattern">{{ pattern[0] }} &rarr; <span class="pattern-rel">{{ pattern[1] }}</span> &rarr; {{ pattern[2] }}</span><span class="count">{{ count }}</span></li>{% endfor %}
                                    </ol>
                                    {% if least_frequent_patterns|length > 25 %}<button class="toggle-button" data-target="least-frequent-list">Show All</button>{% endif %}
                                </div>
                            </div>
                        </div>
                    </div>
                    {% endif %}
                </div>
                </div>
        </div>
    </div>

    <script>
    document.addEventListener('DOMContentLoaded', () => {
        let activeTypeFilters = new Set();
        let activePatternFilter = null;
        let activeRelationFilter = null; 
        let activeSearchTerm = '';

        const allCards = document.querySelectorAll('.extraction-card[data-subject-type]');
        const allTurns = document.querySelectorAll('.speaker-turn');
        const allPatternItems = document.querySelectorAll('.patterns-column li');
        const sidebar = document.getElementById('sidebar');
        const typeFilterContainer = document.getElementById('entity-type-filter-container');
        const patternLists = document.querySelectorAll('.patterns-column ol');
        const clearAllFiltersBtn = document.getElementById('clear-all-filters-btn');
        const searchInput = document.getElementById('entity-search-input');
        
        // Filter Status Display
        const filterStatus = document.getElementById('filter-status');
        const searchFilterDisplay = document.getElementById('search-filter-display');
        const searchFilterText = document.getElementById('search-filter-text');
        const typeFilterDisplay = document.getElementById('type-filter-display');
        const typeFilterTags = document.getElementById('type-filter-tags');
        const patternFilterDisplay = document.getElementById('pattern-filter-display');
        const patternFilterText = document.getElementById('pattern-filter-text');
        const relationFilterDisplay = document.getElementById('relation-filter-display');
        const relationFilterTags = document.getElementById('relation-filter-tags');

        const masterUpdate = () => {
            const hasActiveFilters = activeTypeFilters.size > 0 || activePatternFilter || activeRelationFilter || activeSearchTerm;
            filterStatus.style.display = hasActiveFilters ? 'block' : 'none';

            // Update filter status UI
            document.querySelectorAll('.filterable-badge').forEach(b => b.classList.toggle('selected', activeTypeFilters.has(b.dataset.entityType)));
            document.querySelectorAll('.filterable-relation').forEach(r => r.classList.toggle('selected', activeRelationFilter === r.dataset.relationForm));
            
            // Search filter status
            if (activeSearchTerm) {
                searchFilterDisplay.style.display = 'block';
                searchFilterText.innerHTML = `<code>${activeSearchTerm}</code>`;
            } else {
                searchFilterDisplay.style.display = 'none';
            }

            // Type filter status
            if (activeTypeFilters.size > 0) {
                typeFilterDisplay.style.display = 'block';
                typeFilterTags.innerHTML = '';
                activeTypeFilters.forEach(type => {
                    const originalBadge = typeFilterContainer.querySelector(`.filterable-badge[data-entity-type="${type}"]`);
                    if (originalBadge) {
                        const clone = originalBadge.cloneNode(true);
                        clone.style.display = 'inline-block';
                        clone.classList.remove('filterable-badge', 'selected');
                        clone.style.cursor = 'pointer';
                        clone.title = 'Click to remove this filter';
                        clone.innerHTML += ' &times;';
                        clone.addEventListener('click', () => {
                            activeTypeFilters.delete(type);
                            masterUpdate();
                        });
                        typeFilterTags.appendChild(clone);
                    }
                });
            } else { typeFilterDisplay.style.display = 'none'; }

            // Relation filter status
            if (activeRelationFilter) {
                relationFilterDisplay.style.display = 'block';
                relationFilterTags.innerHTML = `<code>${activeRelationFilter}</code> <span class="remove-filter" data-filter-type="relation">&times;</span>`;
            } else { relationFilterDisplay.style.display = 'none'; }

            // Pattern filter status
            if (activePatternFilter) {
                patternFilterDisplay.style.display = 'block';
                patternFilterText.innerHTML = `<code>${activePatternFilter.s_type} &rarr; <span class="pattern-rel">${activePatternFilter.r_form}</span> &rarr; ${activePatternFilter.o_type}</code>`;
            } else { patternFilterDisplay.style.display = 'none'; }


            // Main Filtering Logic
            allCards.forEach(card => {
                let isVisible = true;
                
                // Text search filter (AND)
                if (isVisible && activeSearchTerm) {
                    const subjName = card.dataset.subjectName || '';
                    const objName = card.dataset.objectName || '';
                    if (!subjName.includes(activeSearchTerm) && !objName.includes(activeSearchTerm)) {
                        isVisible = false;
                    }
                }
                
                // Type filter (AND)
                if (isVisible && activeTypeFilters.size > 0) {
                    if (!activeTypeFilters.has(card.dataset.subjectType) && !activeTypeFilters.has(card.dataset.objectType)) {
                        isVisible = false;
                    }
                }

                // Relation filter (AND)
                if (isVisible && activeRelationFilter) {
                    if (card.dataset.relationForm !== activeRelationFilter) {
                        isVisible = false;
                    }
                }

                // Pattern filter (AND)
                if (isVisible && activePatternFilter) {
                    if (!(card.dataset.subjectType === activePatternFilter.s_type && card.dataset.relationForm === activePatternFilter.r_form && card.dataset.objectType === activePatternFilter.o_type)) {
                        isVisible = false;
                    }
                }
                
                card.classList.toggle('hidden-by-filter', !isVisible);
            });

            // Update visibility of speaker turns
            allTurns.forEach(turn => {
                const hasVisibleCard = turn.querySelector('.extraction-card:not(.hidden-by-filter)');
                turn.classList.toggle('hidden-by-filter', !hasVisibleCard);
            });
            
            // Update visibility of pattern list items based on type filters
            allPatternItems.forEach(item => {
                if (activeTypeFilters.size === 0) { item.classList.remove('hidden-by-filter'); return; }
                const isVisible = activeTypeFilters.has(item.dataset.filterSubject) || activeTypeFilters.has(item.dataset.filterObject);
                item.classList.toggle('hidden-by-filter', !isVisible);
            });
        };

        if (sidebar) {
            sidebar.addEventListener('click', (event) => {
                const target = event.target;

                const badge = target.closest('.filterable-badge');
                if (badge) {
                    const entityType = badge.dataset.entityType;
                    activeTypeFilters.has(entityType) ? activeTypeFilters.delete(entityType) : activeTypeFilters.add(entityType);
                    masterUpdate();
                    return;
                }

                const relation = target.closest('.filterable-relation');
                if (relation) {
                    const relationForm = relation.dataset.relationForm;
                    activeRelationFilter = activeRelationFilter === relationForm ? null : relationForm;
                    masterUpdate();
                    return;
                }

                const collapsibleHeader = target.closest('.collapsible-header');
                if (collapsibleHeader) {
                    collapsibleHeader.closest('.collapsible')?.classList.toggle('collapsed');
                }
                
                if (target.matches('.remove-filter')) {
                    if(target.dataset.filterType === 'relation') { activeRelationFilter = null; }
                    masterUpdate();
                }
            });
        }
        
        patternLists.forEach(list => {
            list.addEventListener('click', (event) => {
                const clickedLi = event.target.closest('li');
                if (!clickedLi || !clickedLi.dataset.filterSubject) return;
                
                const s_type = clickedLi.dataset.filterSubject;
                const r_form = clickedLi.dataset.filterRelation;
                const o_type = clickedLi.dataset.filterObject;
                const isAlreadyActive = activePatternFilter && activePatternFilter.s_type === s_type && activePatternFilter.r_form === r_form && activePatternFilter.o_type === o_type;

                document.querySelectorAll('.active-filter').forEach(el => el.classList.remove('active-filter'));
                
                if (isAlreadyActive) {
                    activePatternFilter = null;
                } else {
                    clickedLi.classList.add('active-filter');
                    activePatternFilter = { s_type, r_form, o_type };
                }
                masterUpdate();
            });
        });
        
        searchInput.addEventListener('input', () => {
            activeSearchTerm = searchInput.value.trim().toLowerCase();
            masterUpdate();
        });

        clearAllFiltersBtn.addEventListener('click', () => {
            activeTypeFilters.clear();
            activePatternFilter = null;
            activeRelationFilter = null;
            activeSearchTerm = '';
            searchInput.value = '';
            document.querySelectorAll('.active-filter').forEach(el => el.classList.remove('active-filter'));
            masterUpdate();
        });

        if (typeFilterContainer) {
            const toggleBtn = document.getElementById('toggle-entity-types-btn');
            const allBadges = typeFilterContainer.querySelectorAll('.filterable-badge');
            const limit = 30;
            if (allBadges.length > limit) {
                toggleBtn.style.display = 'block';
                toggleBtn.textContent = `Show All ${allBadges.length}`;
                for (let i = limit; i < allBadges.length; i++) { allBadges[i].style.display = 'none'; }
                toggleBtn.addEventListener('click', () => {
                    const isExpanded = !toggleBtn.classList.toggle('expanded');
                    for (let i = limit; i < allBadges.length; i++) { allBadges[i].style.display = isExpanded ? 'inline-block' : 'none'; }
                    toggleBtn.textContent = isExpanded ? `Show Less (Top ${limit})` : `Show All ${allBadges.length}`;
                });
            }
        }
        
        document.querySelectorAll('.toggle-button[data-target]').forEach(button => {
             const list = document.getElementById(button.dataset.target);
             if (!list || list.children.length <= 25) { button.style.display = 'none'; return; }
             for (let i = 25; i < list.children.length; i++) { list.children[i].style.display = 'none'; }
             button.textContent = `Show All ${list.children.length}`;
             button.addEventListener('click', () => {
                 const isExpanded = list.classList.toggle('expanded');
                 for (let i = 25; i < list.children.length; i++) { list.children[i].style.display = isExpanded ? 'flex' : 'none'; }
                 button.textContent = isExpanded ? 'Show Less (Top 25)' : `Show All ${list.children.length}`;
             });
        });

        const exportEntityCSVBtn = document.getElementById('export-entity-csv-btn');
        if (exportEntityCSVBtn) {
            exportEntityCSVBtn.addEventListener('click', () => {
                const entityData = {{ all_entity_types | tojson }};
                const csvHeaders = ['entity_type', 'total_frequency', 'utterance_occurrences'];
                const csvRows = entityData.map(row => [`"${row.name.replace(/"/g, '""')}"`, row.count, row.utterance_count]);
                const csvContent = [csvHeaders.join(','), ...csvRows.map(row => row.join(','))].join('\n');
                const blob = new Blob([csvContent], { type: 'text/csv;charset=utf-8;' });
                const link = document.createElement('a');
                const url = URL.createObjectURL(blob);
                link.setAttribute('href', url);
                link.setAttribute('download', 'entity_type_statistics.csv');
                link.style.visibility = 'hidden';
                document.body.appendChild(link);
                link.click();
                document.body.removeChild(link);
                URL.revokeObjectURL(url);
            });
        }
    });
    </script>
</body>
</html>